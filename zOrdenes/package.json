{
    "name": "zordenes",
    "version": "0.0.1",
    "private": true,
    "description": "zOrdenes description",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "devDependencies": {
        "@sap-ux/eslint-plugin-fiori-tools": "^0.2",
        "@sap/ux-ui5-tooling": "^1.19.3",
        "@ui5/cli": "^3.0.0",
        "@ui5/linter": "latest"
    },
    "scripts": {
        "start": "fiori run --open index.html",
        "build": "ui5 build self-contained --all --include-task generateVersionInfo --config=ui5.yaml --clean-dest --dest ../dist/zOrdenes",
        "ui5lint": "ui5lint",
        "lint": "eslint ./",
        "qunit": "fiori run --open test/unitTests.qunit.html",
        "opa5": "fiori run --open test/opaTests.qunit.html"
    }
}
